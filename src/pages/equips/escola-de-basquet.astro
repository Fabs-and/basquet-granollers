---
import TeamsLayout from "@layouts/TeamsLayout.astro";
import { fetchImagesInPageBySlug } from "@utils/apiFunctions/index";

const teamsImages = await fetchImagesInPageBySlug(
  "escola-de-basquet"
);

const title = "Club Bàsquet Granollers";
const heading = "Escola de bàsquet";
---

<TeamsLayout {teamsImages} {title} {heading} />