---
//Import layouts
import TeamsLayout from "@layouts/TeamsLayout.astro";

//Import data
import { maleTeamsImage, maleTeamsTitle } from "@data/apiData";

import { getSeasonYears } from "@utils/helperFunctions";
const { firstYear, secondYear } = getSeasonYears();

const heading = maleTeamsTitle[0].title.rendered;
const title = `Equips masculins temporada 20${firstYear}-${secondYear}`;
---

<TeamsLayout teamsImages={maleTeamsImage} {title} {heading} />
