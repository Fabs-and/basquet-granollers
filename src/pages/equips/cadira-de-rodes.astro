---
import TeamsLayout from "@layouts/TeamsLayout.astro";
import { fetchImagesInPageBySlug } from "fetch-wordpress-api";

const teamsImages = await fetchImagesInPageBySlug(
  "basquet-en-cadira-de-rodes-temporada-22-23/",
);

const title = "L'equip de bàsquet en cadira de rodes del club Bàsquet Granollers";
const heading = "L'equip de bàsquet en cadira de rodes";
---

<TeamsLayout {teamsImages} {title} {heading} />
