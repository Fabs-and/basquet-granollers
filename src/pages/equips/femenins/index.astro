---
//Import layouts
import TeamsLayout from "@layouts/TeamsLayout.astro";

//Import data
import { femaleTeamsImage, femaleTeamsTitle } from "@data/apiData";
import { getSeasonYears } from "@utils/helperFunctions";
const { firstYear, secondYear } = getSeasonYears();

const heading = femaleTeamsTitle[0].title.rendered;
const title = `Equips femenins temporada 20${firstYear}-${secondYear}`;
---

<TeamsLayout teamsImages={femaleTeamsImage} {title} {heading} />
