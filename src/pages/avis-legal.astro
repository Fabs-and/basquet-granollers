---
import { fetchPageBySlug, fetchPages } from 'fetch-wordpress-api'

import BaseLayout from '@layouts/BaseLayout.astro';
import PostLayout from '@layouts/PostLayout.astro';

// import { Endpoints, PostFields } from 'src/types';

const [page] = await fetchPageBySlug('avis-legal')

const post = {
  title: page.title.rendered,
  content: page.content.rendered
}
---

<BaseLayout title={post.title}>
  <PostLayout {post}/>
</BaseLayout>


